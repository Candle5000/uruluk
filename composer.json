{
    "name": "candle5000/uruluk",
    "description": "Uruluk - TRIGLAV 攻略サイト",
    "keywords": [
        "TRIGLAV",
        "Candle5000"
    ],
    "homepage": "http://github.com/Candle5000/uruluk",
    "license": "MIT",
    "authors": [
        {
            "name": "Candle Doe"
        }
    ],
    "require": {
        "php": ">=7.3",
        "monolog/monolog": "^1.17",
        "slim/csrf": "^0.8.3",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "zonuexe/http-accept-language": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Controller\\": "src/classes/controller/",
            "Model\\": "src/classes/model/",
            "I18n\\": "src/classes/i18n/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
